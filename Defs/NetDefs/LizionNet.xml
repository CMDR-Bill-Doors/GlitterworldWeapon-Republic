<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	<DesignatorDropdownGroupDef>
		<defName>Group_BDP_Lizionpipe</defName>
		<label>Lizion pipes</label>
	</DesignatorDropdownGroupDef>

	<PipeSystem.PipeNetDef>
		<defName>BDP_LizionNetwork</defName>
		<resource>
			<name>lizion</name>
			<unit>L</unit>
			<color>(249, 157, 88)</color>
			<onlyShowStored>True</onlyShowStored>
			<offTexPath>Things/Item/Resource/Chemfuel</offTexPath>
		</resource>
		<pipeDefs>
			<li>BDP_LizionPipe</li>
			<li>BDP_LizionPipeUnderground</li>
		</pipeDefs>
		<designator>
			<deconstructIconPath>ui/designators/Deconstruct</deconstructIconPath>
			<designationCategoryDef>Structure</designationCategoryDef>
		</designator>
		<overlayOptions>
			<overlayColor>(149, 99, 60)</overlayColor>
		</overlayOptions>
	</PipeSystem.PipeNetDef>

	<ThingDef ParentName="BuildingBase">
		<defName>BDP_LizionPipe</defName>
		<label>lizion pipe</label>
		<description>Heat insulating pipes used to carry cryogenic lizion gas.</description>
		<thingClass>PipeSystem.Building_Pipe</thingClass>
		<category>Building</category>
		<tickerType>Rare</tickerType>
		<graphicData>
			<texPath>Things/Building/BDP_LizionPipe_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkType>Basic</linkType>
			<linkFlags>
				<li>Custom10</li>
			</linkFlags>
			<damageData>
				<rect>(0,0.35,1,0.3)</rect>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/BDP_LizionPipe_Icon</uiIconPath>
		<designatorDropdown>Group_BDP_Lizionpipe</designatorDropdown>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<isInert>true</isInert>
			<isEdifice>false</isEdifice>
			<blueprintGraphicData>
				<texPath>Things/Building/BDP_LizionPipe_Blueprint_Atlas</texPath>
			</blueprintGraphicData>
		</building>
		<drawerType>MapMeshOnly</drawerType>
		<altitudeLayer>Conduits</altitudeLayer>
		<passability>Standable</passability>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<statBases>
			<MaxHitPoints>40</MaxHitPoints>
			<WorkToBuild>35</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<costList>
			<Steel>2</Steel>
		</costList>
		<comps>
			<li Class="PipeSystem.CompProperties_Resource">
				<pipeNet>BDP_LizionNetwork</pipeNet>
			</li>
			<li Class="PipeSystem.CompProperties_SpillWhenDamaged">
				<spillEachTicks>600</spillEachTicks>
				<startAtHitPointsPercent>0.333</startAtHitPointsPercent>
				<amountToDraw>1</amountToDraw>
				<filthMaxSpawnRadius>2</filthMaxSpawnRadius>
				<chooseFilthFrom>
					<li>Filth_Water</li>
				</chooseFilthFrom>
			</li>
		</comps>
		<placeWorkers>
			<li>PipeSystem.PlaceWorker_Pipe</li>
		</placeWorkers>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<designationCategory>Structure</designationCategory>
		<constructEffect>ConstructMetal</constructEffect>
		<clearBuildingArea>false</clearBuildingArea>
		<researchPrerequisites>
			<li>BiofuelRefining</li>
		</researchPrerequisites>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>BDP_LizionPipeUnderground</defName>
		<label>underground lizion pipe</label>
		<description>Underground heat insulating pipes used to carry cryogenic lizion gas.</description>
		<thingClass>PipeSystem.Building_Pipe</thingClass>
		<category>Building</category>
		<tickerType>Rare</tickerType>
		<graphicData>
			<texPath>UI/CSG/IConduit</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkType>Basic</linkType>
			<linkFlags>
				<li>Custom10</li>
			</linkFlags>
		</graphicData>
		<uiIconPath>Things/Building/BDP_LizionPipe_Icon_Underground</uiIconPath>
		<designatorDropdown>Group_BDP_Lizionpipe</designatorDropdown>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<isInert>true</isInert>
			<isEdifice>false</isEdifice>
			<blueprintGraphicData>
				<texPath>Things/Building/BDP_LizionPipe_Blueprint_Atlas</texPath>
			</blueprintGraphicData>
		</building>
		<drawerType>MapMeshOnly</drawerType>
		<altitudeLayer>Conduits</altitudeLayer>
		<passability>Standable</passability>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<statBases>
			<MaxHitPoints>400</MaxHitPoints>
			<WorkToBuild>120</WorkToBuild>
			<Flammability>0</Flammability>
		</statBases>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<costList>
			<Steel>4</Steel>
		</costList>
		<comps>
			<li Class="PipeSystem.CompProperties_Resource">
				<pipeNet>BDP_LizionNetwork</pipeNet>
			</li>
			<li Class="PipeSystem.CompProperties_SpillWhenDamaged">
				<spillEachTicks>12</spillEachTicks>
				<startAtHitPointsPercent>0.333</startAtHitPointsPercent>
				<amountToDraw>1</amountToDraw>
				<filthMaxSpawnRadius>2</filthMaxSpawnRadius>
				<chooseFilthFrom>
					<li>Filth_Water</li>
				</chooseFilthFrom>
			</li>
		</comps>
		<placeWorkers>
			<li>PipeSystem.PlaceWorker_Pipe</li>
		</placeWorkers>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<designationCategory>Structure</designationCategory>
		<constructEffect>ConstructMetal</constructEffect>
		<clearBuildingArea>false</clearBuildingArea>
		<researchPrerequisites>
			<li>BiofuelRefining</li>
		</researchPrerequisites>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>BDP_LizionTankSmall</defName>
		<label>small lizion tank</label>
		<description>A small, movable tank designed to store liquid lizion.</description>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/LizionTank_Small</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2,2)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<uiIconScale>0.7</uiIconScale>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
		<blockWind>false</blockWind>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>0.40</fillPercent>
		<tickerType>Normal</tickerType>
		<rotatable>false</rotatable>
		<statBases>
			<MaxHitPoints>150</MaxHitPoints>
			<WorkToBuild>800</WorkToBuild>
			<Mass>10</Mass>
			<Flammability>0.1</Flammability>
		</statBases>
		<size>(1,1)</size>
		<costList>
			<Steel>140</Steel>
		</costList>
		<comps>
			<li Class="PipeSystem.CompProperties_ResourceStorage">
				<pipeNet>BDP_LizionNetwork</pipeNet>
				<storageCapacity>500</storageCapacity>
				<barSize>(0.29, 0.09)</barSize>
				<margin>0.05</margin>
				<centerOffset>(-0.067, 0, -0.24)</centerOffset>
			</li>
			<li Class="PipeSystem.CompProperties_ExplosiveContent">
				<explosiveMaxRadius>12</explosiveMaxRadius>
				<explosiveDamageType>Flame</explosiveDamageType>
				<destroyThingOnExplosionSize>2</destroyThingOnExplosionSize>
				<startWickHitPointsPercent>0.333</startWickHitPointsPercent>
				<preExplosionSpawnThingDef>Filth_Fuel</preExplosionSpawnThingDef>
				<preExplosionSpawnChance>1</preExplosionSpawnChance>
				<wickTicks>70~150</wickTicks>
			</li>
		</comps>
		<building>
			<relatedBuildCommands>
				<li>BDP_LizionPipe</li>
				<li>BDP_LizionPipeUnderground</li>
			</relatedBuildCommands>
		</building>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<designationCategory>Production</designationCategory>
		<staticSunShadowHeight>0.2</staticSunShadowHeight>
		<constructEffect>ConstructMetal</constructEffect>
		<canOverlapZones>false</canOverlapZones>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<researchPrerequisites>
			<li>BiofuelRefining</li>
		</researchPrerequisites>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>BDP_LizionRefinerySmall</defName>
		<label>small lizion extractor</label>
		<description>A serie of condensors, supercoolers, centifuge and nanomembranes extracts the trace amount of lizion withing the atmosphere</description>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/BDP_SmallLizionRefinery</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2,2)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<rotatable>false</rotatable>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<pathCost>50</pathCost>
		<blockWind>true</blockWind>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>0.40</fillPercent>
		<tickerType>Normal</tickerType>
		<statBases>
			<MaxHitPoints>250</MaxHitPoints>
			<WorkToBuild>1000</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<size>(2,2)</size>
		<costList>
			<Steel>200</Steel>
			<ComponentIndustrial>4</ComponentIndustrial>
		</costList>
		<comps>
			<li Class="PipeSystem.CompProperties_ResourceProcessor">
				<pipeNet>BDP_AirNetwork</pipeNet>
				<showBufferInfo>false</showBufferInfo>
				<results>
					<li>
						<countNeeded>100</countNeeded>
						<eachTicks>300</eachTicks>

						<thing>Gas_Lizion</thing>
						<thingCount>3</thingCount>

						<net>BDP_LizionNetwork</net>
						<netCount>1</netCount>
					</li>
				</results>
				<soundAmbient>ChemfuelFiredGenerator_Ambience</soundAmbient>
				<notWorkingKey>BDP_CantRefine</notWorkingKey>
			</li>
			<li Class="PipeSystem.CompProperties_Resource">
				<pipeNet>BDP_LizionNetwork</pipeNet>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>600</basePowerConsumption>
				<shortCircuitInRain>false</shortCircuitInRain>
			</li>
		</comps>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<designationCategory>Production</designationCategory>
		<staticSunShadowHeight>0.2</staticSunShadowHeight>
		<constructEffect>ConstructMetal</constructEffect>
		<canOverlapZones>false</canOverlapZones>
		<researchPrerequisites>
			<li>BiofuelRefining</li>
		</researchPrerequisites>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>BDP_LizionFillerSmall</defName>
		<label>small lizion filling machine</label>
		<description>A serie of condensors, supercoolers, centifuge and nanomembranes extracts the trace amount of lizion withing the atmosphere</description>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/BDP_SmallLizionFiller</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2,2)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<rotatable>true</rotatable>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<pathCost>50</pathCost>
		<blockWind>true</blockWind>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>0.40</fillPercent>
		<tickerType>Normal</tickerType>
		<statBases>
			<Mass>10</Mass>
			<MaxHitPoints>250</MaxHitPoints>
			<WorkToBuild>1000</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<size>(1,1)</size>
		<costList>
			<Steel>200</Steel>
			<ComponentIndustrial>4</ComponentIndustrial>
		</costList>
		<comps>
			<li Class="BDsPlasmaWeapon.CompProperties_LizionCellFiller">
				<pipeNet>BDP_LizionNetwork</pipeNet>
				<showBufferInfo>false</showBufferInfo>
				<consumptionPerTick>0</consumptionPerTick>
				<results>
					<li>
						<countNeeded>1</countNeeded>
						<eachTicks>60</eachTicks>
						<thing>Ammo_LizionCellNormal</thing>
						<thingCount>1</thingCount>
					</li>
					<li>
						<countNeeded>1</countNeeded>
						<eachTicks>60</eachTicks>
						<thing>Ammo_LizionCellInert</thing>
						<thingCount>2</thingCount>
					</li>
					<li>
						<countNeeded>4</countNeeded>
						<eachTicks>120</eachTicks>
						<thing>Ammo_LizionCellOvercharged</thing>
						<thingCount>1</thingCount>
					</li>
				</results>
				<soundAmbient>ChemfuelFiredGenerator_Ambience</soundAmbient>
				<notWorkingKey>BDP_CantRefine</notWorkingKey>
			</li>
			<li Class="PipeSystem.CompProperties_Resource">
				<pipeNet>BDP_LizionNetwork</pipeNet>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>10</basePowerConsumption>
				<shortCircuitInRain>false</shortCircuitInRain>
			</li>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>0</fuelConsumptionRate>
				<fuelCapacity>100</fuelCapacity>
				<autoRefuelPercent>0.5</autoRefuelPercent>
				<showFuelGizmo>true</showFuelGizmo>
				<fuelFilter>
					<thingDefs>
						<li>BDP_EmptyLizionCell</li>
					</thingDefs>
				</fuelFilter>
				<initialFuelPercent>0</initialFuelPercent>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
			</li>
		</comps>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<designationCategory>Production</designationCategory>
		<staticSunShadowHeight>0.2</staticSunShadowHeight>
		<constructEffect>ConstructMetal</constructEffect>
		<canOverlapZones>false</canOverlapZones>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<researchPrerequisites>
			<li>BiofuelRefining</li>
		</researchPrerequisites>
	</ThingDef>
</Defs>